{
  "name": "seneca-basic",
  "version": "0.4.0",
  "description": "Seneca utility plugin.",
  "main": "basic.js",
  "scripts": {
    "test": "lab test/*.test.js -v -r console -t 80 $LAB_FLAGS",
    "test:node6": "LAB_FLAGS='--leaks' && npm test",
    "build": "./build.sh",
    "annotate": "docco basic.js -o doc",
    "coverage": "lab test/*.test.js -v -r lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/seneca-basic"
  },
  "keywords": [
    "seneca",
    "plugin",
    "util",
    "utility",
    "basic"
  ],
  "author": "Richard Rodger (http://richardrodger.com)",
  "contributors": [
    "Richard Rodger (http://richardrodger.com)",
    "Bogdan Rotund"
  ],
  "license": "MIT",
  "dependencies": {
    "async": "1.5.2",
    "lodash": "4.0.0",
    "nid": "0.3.2"
  },
  "devDependencies": {
    "seneca": "plugin",
    "seneca-entity": "0.0.1",
    "docco": "0.6.3",
    "eslint-config-seneca": "1.1.2",
    "eslint-plugin-standard": "1.3.1",
    "lab": "6.2.0",
    "coveralls": "2.11.x"
  },
  "files": [
    "LICENSE.txt",
    "README.md",
    "basic.js",
    "lib/common.js",
    "lib/entity.js",
    "lib/note.js",
    "lib/uniqueid.js"
  ]
}
